{
  "name": "let-s-go",
  "private": true,
  "description": "Let-s-go",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "^10.4.1",
    "browser-sync": "^2.27.7",
    "gulp": "4.0.2",
    "gulp-dart-sass": "^1.0.2",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-sourcemaps": "3.0.0",
    "postcss": "^8.4.5",
    "postcss-scss": "^4.0.2",
    "stylelint": "^14.2.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --custom-syntax postcss-scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp styles",
    "start": "gulp"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.scss"
  ],
  "engines": {
    "node": "^12.x || ^16.x"
  }
}
