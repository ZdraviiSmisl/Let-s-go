{
  "name": "let-s-go",
  "private": true,
  "description": "Let-s-go",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "^10.4.1",
    "browser-sync": "^2.27.7",
    "del": "^6.1.1",
    "gulp": "4.0.2",
    "gulp-dart-sass": "^1.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-libsquoosh": "^1.1.2",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-terser": "^2.1.0",
    "gulp-webp": "^4.0.1",
    "postcss": "^8.4.5",
    "postcss-csso": "^6.0.0",
    "postcss-import": "^14.1.0",
    "postcss-scss": "^4.0.4",
    "postcss-url": "^10.1.3",
    "prettier": "2.5.1",
    "stylelint": "^14.2.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --custom-syntax postcss-scss",
